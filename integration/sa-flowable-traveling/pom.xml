<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
   <modelVersion>4.0.0</modelVersion>
   <parent>
    <artifactId>parent</artifactId>
    <groupId>org.ow2.petals.integration.tests.se.flowable</groupId>
    <version>5.2.0-SNAPSHOT</version>
    <relativePath>../parent/pom.xml</relativePath>
  </parent>
  
  <artifactId>sa-flowable-traveling</artifactId>
  <packaging>jbi-service-assembly</packaging>
  <name>Petals Integration Tests - Tests - Components - Petals SE Flowable - SA (the traveling process)</name>
  <description>sa-flowable-traveling description</description>
  <!-- No version set -->
   
   <properties>
      <!-- Skip dependency check for service assembly because it has Java no source code. -->
      <maven-dependency-plugin.failOnWarning>false</maven-dependency-plugin.failOnWarning>

      <jbi-sa.build.legal-dir>${project.build.directory}/legal-additionals</jbi-sa.build.legal-dir>
   </properties>

   <dependencies>
      <dependency>
         <groupId>org.ow2.petals.samples.flowable</groupId>
         <artifactId>su-flowable-travelingService-provide</artifactId>
         <version>${petals-samples-flowable.version}</version>
         <type>jbi-service-unit</type>
      </dependency>
      <dependency>
         <groupId>org.ow2.petals.samples.flowable</groupId>
         <artifactId>su-SOAP-travelingService-consume</artifactId>
         <version>${petals-samples-flowable.version}</version>
         <type>jbi-service-unit</type>
      </dependency>
      <dependency>
         <groupId>org.ow2.petals.samples.flowable</groupId>
         <artifactId>su-SOAP-notifyTravelingService-provide</artifactId>
         <version>${petals-samples-flowable.version}</version>
         <type>jbi-service-unit</type>
      </dependency>
      <dependency>
         <groupId>org.ow2.petals.samples.flowable</groupId>
         <artifactId>su-SOAP-notifyTravelingService-consume</artifactId>
         <version>${petals-samples-flowable.version}</version>
         <type>jbi-service-unit</type>
      </dependency>
      <dependency>
         <groupId>org.ow2.petals.samples.flowable</groupId>
         <artifactId>su-SOAP-bookingService-provide</artifactId>
         <version>${petals-samples-flowable.version}</version>
         <type>jbi-service-unit</type>
      </dependency>
      <dependency>
         <groupId>org.ow2.petals.samples.flowable</groupId>
         <artifactId>su-SOAP-bookingService-consume</artifactId>
         <version>${petals-samples-flowable.version}</version>
         <type>jbi-service-unit</type>
      </dependency>
   </dependencies>

   <build>
      <plugins>
         <plugin>
            <groupId>org.ow2.petals</groupId>
            <artifactId>petals-maven-plugin</artifactId>
            <configuration>
               <serviceAssemblyNameMapping>${project.artifactId}</serviceAssemblyNameMapping>
            </configuration>
         </plugin>
      </plugins>
   </build>

   <scm>
      <connection>scm:git:https://github.com/petalslink/petals-se-flowable.git</connection>
      <developerConnection>scm:git:https://github.com/petalslink/petals-se-flowable.git</developerConnection>
     <tag>HEAD</tag>
  </scm>
</project>
